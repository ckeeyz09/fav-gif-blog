<div class="col-sm-4 col-sm-offset-4">
  <legend>Log in post your gifs!</legend>
  <%= form_for :user do |f| %>
     <%= f.label :email %>:
     <%= f.text_field :email, class:"form-control", required: true %><br />

     <%= f.label :password %>:
     <%= f.text_field :password_digest, class:"form-control", required: true %><br />

     <%= f.submit "Log In", class:"btn btn-primary" %>
  <% end %>
</div>